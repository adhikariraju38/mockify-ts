{
  "name": "mockify-ts",
  "version": "0.1.0",
  "description": "Lightweight TypeScript mock data factory with smart field inference for Jest/Vitest and React Testing Library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mock",
    "test",
    "factory",
    "typescript",
    "jest",
    "vitest",
    "testing",
    "fake-data",
    "test-data",
    "react-testing-library",
    "mock-data",
    "test-factory",
    "type-safe"
  ],
  "author": "Raju Kumar Yadav <notrajuyadav@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adhikariraju38/mockify-ts.git"
  },
  "bugs": {
    "url": "https://github.com/adhikariraju38/mockify-ts/issues"
  },
  "homepage": "https://github.com/adhikariraju38/mockify-ts#readme",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
